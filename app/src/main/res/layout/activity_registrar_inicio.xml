<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".RegistrarInicio">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="130dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="430dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logotaskwise" />


    <EditText
        android:id="@+id/editTextCorreo"
        android:layout_width="360dp"
        android:layout_height="56dp"
        android:layout_marginBottom="312dp"
        android:background="@drawable/background_rounded_edittext"
        android:hint="   Iniciar sesion con Gmail"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextCorreo2"
        android:layout_width="360dp"
        android:layout_height="56dp"
        android:layout_marginBottom="232dp"
        android:background="@drawable/background_rounded_edittext"
        android:hint="   Iniciar sesion con Outllok"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="44dp"
        android:layout_height="37dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextCorreo"
        app:layout_constraintEnd_toEndOf="@+id/editTextCorreo"
        app:layout_constraintTop_toTopOf="@+id/editTextCorreo"
        app:layout_constraintVertical_bias="0.526"
        app:srcCompat="@drawable/gmail_logo" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="44dp"
        android:layout_height="37dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextCorreo2"
        app:layout_constraintEnd_toEndOf="@+id/editTextCorreo2"
        app:layout_constraintTop_toTopOf="@+id/editTextCorreo2"
        app:layout_constraintVertical_bias="0.473"
        app:srcCompat="@drawable/microsoft_outlook_logo_wine" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#025CA3"
        android:text="Iniciar Sesion"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextCorreo2" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="88dp"
        android:layout_height="21dp"
        android:layout_marginStart="36dp"
        android:text="Iniciar Sesion"
        app:layout_constraintBottom_toTopOf="@+id/editTextCorreo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.512" />

</androidx.constraintlayout.widget.ConstraintLayout>